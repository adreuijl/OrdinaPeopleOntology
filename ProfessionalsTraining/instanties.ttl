instantie:
  a owl:Ontology ;
  spin:query [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "s" ;
          ]
          [
            sp:varName "p" ;
          ]
          [
            sp:varName "o" ;
          ]
        ) ;
      sp:where (
          [
            sp:object ontologie:Persoon ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "s" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "o" ;
              ] ;
            sp:predicate [
                sp:varName "p" ;
              ] ;
            sp:subject [
                sp:varName "s" ;
              ] ;
          ]
          [
            a sp:TriplePath ;
            sp:object ontologie:heeft ;
            sp:path [
                a sp:ModPath ;
                sp:modMax -2 ;
                sp:modMin 0 ;
                sp:subPath rdfs:subPropertyOf ;
              ] ;
            sp:subject [
                sp:varName "p" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:query [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "s" ;
          ]
          [
            sp:varName "p" ;
          ]
          [
            sp:varName "o" ;
          ]
        ) ;
      sp:where (
          [
            sp:object ontologie:Persoon ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "s" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "o" ;
              ] ;
            sp:predicate [
                sp:varName "p" ;
              ] ;
            sp:subject [
                sp:varName "s" ;
              ] ;
          ]
          [
            a sp:TriplePath ;
            sp:object <http://xmlns.com/foaf/0.1/knows> ;
            sp:path [
                a sp:ModPath ;
                sp:modMax -2 ;
                sp:modMin 0 ;
                sp:subPath rdfs:subPropertyOf ;
              ] ;
            sp:subject [
                sp:varName "p" ;
              ] ;
          ]
        ) ;
    ] ;
  owl:imports ontologie: ;
  owl:imports <http://spinrdf.org/spin> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
