ontologie:Persoon
  spin:query [
      a sp:Select ;
      sp:distinct true ;
      sp:resultVariables (
          [
            sp:varName "persoon" ;
          ]
        ) ;
      sp:where (
          [
            a sp:Union ;
            sp:elements (
                (
                  [
                    sp:object [
                        sp:varName "dbpPerson" ;
                      ] ;
                    sp:predicate owl:equivalentClass ;
                    sp:subject ontologie:Persoon ;
                  ]
                  [
                    sp:object ontologie:Persoon ;
                    sp:predicate rdf:type ;
                    sp:subject [
                        sp:varName "persoon" ;
                      ] ;
                  ]
                  [
                    a sp:Optional ;
                    sp:elements (
                        [
                          sp:object [
                              sp:varName "activiteit" ;
                            ] ;
                          sp:predicate ontologie:beoefent ;
                          sp:subject [
                              sp:varName "x" ;
                            ] ;
                        ]
                        [
                          sp:object [
                              sp:varName "activiteitType" ;
                            ] ;
                          sp:predicate ontologie:heeftType ;
                          sp:subject [
                              sp:varName "activiteit" ;
                            ] ;
                        ]
                      ) ;
                  ]
                )
                (
                  [
                    a sp:Service ;
                    sp:elements (
                        [
                          sp:object [
                              sp:varName "dbpPerson" ;
                            ] ;
                          sp:predicate rdf:type ;
                          sp:subject [
                              sp:varName "persoon" ;
                            ] ;
                        ]
                        [
                          sp:object <http://xmlns.com/foaf/0.1/Person> ;
                          sp:predicate rdf:type ;
                          sp:subject [
                              sp:varName "persoon" ;
                            ] ;
                        ]
                      ) ;
                    sp:serviceURI <http://dbpedia.org/sparql> ;
                  ]
                )
              ) ;
          ]
        ) ;
    ] ;
  spin:query [
      a sp:Select ;
      sp:distinct true ;
      sp:where (
          [
            sp:object ontologie:Persoon ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "persoon" ;
              ] ;
          ]
          [
            a sp:TriplePath ;
            sp:object [
                sp:varName "KennisVanKennis" ;
              ] ;
            sp:path [
                a sp:SeqPath ;
                sp:path1 <http://xmlns.com/foaf/0.1/knows> ;
                sp:path2 <http://xmlns.com/foaf/0.1/knows> ;
              ] ;
            sp:subject [
                sp:varName "persoon" ;
              ] ;
          ]
          [
            a sp:Filter ;
            sp:expression [
                a sp:ne ;
                sp:arg1 [
                    sp:varName "KennisVanKennis" ;
                  ] ;
                sp:arg2 [
                    sp:varName "persoon" ;
                  ] ;
              ] ;
          ]
        ) ;
    ] ;
  owl:equivalentClass <http://xmlns.com/foaf/0.1/Person> ;
